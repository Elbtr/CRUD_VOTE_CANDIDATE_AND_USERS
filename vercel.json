{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node@16.20.2"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": ".*",
      "dest": "src/index.ts"
    }
  ],

  "rewrites": [{ "source": "/api/(.*)", "destination": "/" }]
}
